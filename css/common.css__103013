#floating_window_text
{
	position:absolute;
	left:10px;
	top:10px;
	width:auto;
	height:auto;
	color:#FFF;
	font-family:GothamHTF;
	font-size:1.1em;
}

#dimension_test
{
	position:absolute;
	left:-1000px;
	top:-1000px;
	width:auto;
	height:auto;
	color:#FFF;
	font-family:GothamHTF;
	font-size:1.1em;
}

#prompt {
	visibility:hidden;
	position:absolute;
	left:0px;
	top:0px;
	width:100%;
	height:100%;
	background:rgba(0,0,0,0.25);
}

#prompt_content {
	position:absolute;
	left:50%;
	top:50%;
	width:350px;
	height:175px;
	margin:  -87px 0px 0px -175px;
	background:#393939;
	border:1px solid #FFF;
	padding:10px;
}

#prompt_text {
	width:100%;
	/*background:#00F;*/
	font-family:GothamHTF;
	font-size:1.1em;
	color: #FFFFFF;
}

#prompt_yes_btn {
	display:inline-block;
}

#prompt_no_btn {
	display:inline-block;
}

#prompt_other_btn {
	display:inline-block;
}

.skinned_button {
	width:auto;
	height:auto;
	border: 1px solid #FFF;
	background: #7D110C;
	padding: 3px 6px 3px 6px;
}

.button_text {
	display:inline-block;
	width:auto;
	height:auto;
	font-size:1.3em;
	color:#FFF;
	font-family:Vitesse;
	margin:0px;
}

.button_skin {
	display:inline-block;
	background: url('../img/ui_sprite.png') no-repeat -459px -182px; 
	width: 14px; 
	height: 13px;
	margin-left:5px;
	margin-top:1px;
}

#pano_locator {
	pointer-events:none;
	visibility:hidden;
	position:absolute;
	left:0px;
	top:0px;
	background: url('../img/ui_sprite.png') no-repeat -496px -135px; 
	width:27px; 
	height:40px;
	margin-left:-14px;
	margin-top:-40px;
}

#template_container {
	display:none;
	position:absolute;
	left:0px;
	top:0px;
}