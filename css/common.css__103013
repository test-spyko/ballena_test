	left:145px;
	top:160px;
	width:310px;
	font-weight:bold;
	font-family:sans-serif;
	font-size:.9em;
	color:#FFFFFF;
	text-shadow: 2px 2px #000;
}

#floating_window {
	visibility:hidden;
	position:absolute;
	/*
	left:50%;
	top:50%;
	margin: -90px 0 0 -125px;
	*/
	right:0px;
	bottom:0px;
	width:200px;
	height:200px;
	background:#333;
	border-top:2px solid #FFF;
	border-left:2px solid #FFF;
	border-radius:15px 0 0 0;
}

.seat_description>h2 {
	color:#FFF;
	font-family:GothamHTF;
	font-size:1.3em;
}

.seat_description>h3 {
	color:#FFF;
	font-family:GothamHTF;
	font-size:1.1em;
}

#seat_description
{
	position:absolute;
	left:10px;
	top:10px;
	width:180px;
	height:180px;
}

#floating_window_section {
	width:33%;
	height:50px;
	position:absolute;
	left:0px;
	top:0px;
	text-align:center;
	border-bottom: 2px solid #FFF;
	border-right: 2px solid #FFF;
}

#floating_window_row {
	width:34%;
	height:50px;
	position:absolute;
	left:33%;
