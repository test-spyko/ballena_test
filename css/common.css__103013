	text-align:center;
	border-bottom: 2px solid #FFF;
}


#floating_window_seat {
	width:33%;
	height:50px;
	position:absolute;
	right:0px;
	top:0px;
	text-align:center;
	border-bottom: 2px solid #FFF;
	border-left: 2px solid #FFF;
}

#floating_window_ticket {
	width:50%;
